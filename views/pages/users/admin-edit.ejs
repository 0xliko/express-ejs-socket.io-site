<div class="modal fade" id="edit-modal" tabindex="-1" role="dialog" aria-labelledby="myCenterModalLabel"
     aria-hidden="true"
     style="display: none;">
    <div class="modal-dialog modal-dialog-centered">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title edit-modal-title" id="modal-title">Center modal</h5>
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
            </div>
            <div class="modal-body">
                <form class="form">
                    <input type="hidden" name="_id"/>
                    <div class="form-group">
                        <label for="name">اسم المشرف</label>
                        <input type="text" class="form-control" name="name" required>
                    </div>
                    <div class="form-group">
                        <label for="email">البريد الالكتروني</label>
                        <input type="email" parsley-type="email" class="form-control" name="email" required>
                    </div>
                    <div class="form-group">
                        <label for="sheet_num">عدد ورقة</label>
                        <input type="number" class="form-control" name="sheet_num" required>
                    </div>
                    <div class="form-group">
                        <label for="password">كلمه السر</label>
                        <input type="password" class="form-control" data-parsley-iff="#confirm" id="password"
                               name="password" required>

                        <label for="confirm">أعد الدخول</label>
                        <input type="password" data-parsley-iff="#password" class="form-control" id="confirm"
                               name="confirm" required>
                    </div>
                    <button type="button" class="btn btn-primary btn-save">حفظ</button>
                </form>
            </div>
        </div>
    </div>
</div>
